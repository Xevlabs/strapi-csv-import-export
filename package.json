{
  "name": "strapi-csv-import-export",
  "version": "0.0.7-b",
  "description": "Import export with csv for strapi 5",
  "keywords": [],
  "license": "MIT",
  "author": "Xev <contact@xev.agency>",
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "verify": "strapi-plugin verify",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@uiw/react-codemirror": "^4.25.3",
    "csvtojson": "^2.0.14",
    "joi": "^17.13.3",
    "json-2-csv": "^5.5.10",
    "node-fetch": "^3.3.2",
    "react-intl": "^6.8.9",
    "react-singleton-hook": "^4.0.1"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.32.0",
    "@strapi/typescript-utils": "^5.32.0",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "prettier": "^3.7.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.2",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "styled-components": "^6.1.13"
  },
  "strapi": {
    "kind": "plugin",
    "name": "strapi-csv-import-export",
    "displayName": "Import Export CSV",
    "description": "Import export with CSV for strapi 5"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xevlabs/strapi-csv-import-export.git"
  },
  "bugs": {
    "url": "https://github.com/Xevlabs/strapi-csv-import-export/issues"
  },
  "homepage": "https://github.com/Xevlabs/strapi-csv-import-export#readme"
}
